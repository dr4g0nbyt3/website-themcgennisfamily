  <!-- JavaScript for filtering and load more -->
  <script>
    // Category filtering
    const filterButtons = document.querySelectorAll('.category-filter');
    const blogPosts = document.querySelectorAll('.blog-post');
    const featuredPost = document.querySelector('[data-category="business"]');

    // Category color mapping
    const categoryColors = {
      'all': { bg: 'bg-mcgennis-navy', text: 'text-mcgennis-navy', border: 'border-mcgennis-navy' },
      'business': { bg: 'bg-mcgennis-navy', text: 'text-mcgennis-navy', border: 'border-mcgennis-navy' },
      'ministry': { bgStyle: '#5b2c6f', textStyle: '#5b2c6f', borderStyle: '#5b2c6f' },
      'technology': { bg: 'bg-mcgennis-gold', text: 'text-mcgennis-gold', border: 'border-mcgennis-gold' },
      'personal': { bg: 'bg-mcgennis-maroon', text: 'text-mcgennis-maroon', border: 'border-mcgennis-maroon' }
    };

    filterButtons.forEach(button => {
      button.addEventListener('click', () => {
        const category = button.dataset.category;

        // Reset all buttons to default state
        filterButtons.forEach(btn => {
          const btnCategory = btn.dataset.category;
          const colors = categoryColors[btnCategory];

          btn.classList.remove('active');
          btn.classList.remove('bg-mcgennis-navy', 'bg-mcgennis-gold', 'bg-mcgennis-maroon');
          btn.classList.add('bg-white');

          // Apply category-specific colors for default state
          if (colors.bg) {
            btn.classList.remove('text-white');
            btn.classList.add(colors.text);
          } else if (colors.textStyle) {
            btn.style.backgroundColor = 'white';
            btn.style.color = colors.textStyle;
          }
        });

        // Apply active state to clicked button
        const activeColors = categoryColors[category];
        button.classList.add('active');
        button.classList.remove('bg-white');

        if (activeColors.bg) {
          button.classList.add(activeColors.bg, 'text-white');
          button.classList.remove(activeColors.text);
        } else if (activeColors.bgStyle) {
          button.style.backgroundColor = activeColors.bgStyle;
          button.style.color = 'white';
        }

        // Filter posts
        blogPosts.forEach(post => {
          if (category === 'all' || post.dataset.category === category) {
            post.style.display = 'block';
            // Remove hidden class if it was added
            post.classList.remove('hidden');
          } else {
            post.style.display = 'none';
          }
        });

        // Show featured post only if category matches or "all"
        if (featuredPost) {
          if (category === 'all' || featuredPost.dataset.category === category) {
            featuredPost.closest('section').style.display = 'block';
          } else {
            featuredPost.closest('section').style.display = 'none';
          }
        }

        // Reset load more
        resetLoadMore();
      });
    });

    // Load More functionality
    const loadMoreBtn = document.getElementById('load-more-btn');
    const hiddenPosts = document.querySelectorAll('.blog-post.hidden');

    function resetLoadMore() {
      // Hide the button if all posts are visible or filtered
      const visiblePosts = Array.from(blogPosts).filter(post => post.style.display !== 'none');
      const hiddenVisiblePosts = visiblePosts.filter(post => post.classList.contains('hidden'));

      if (hiddenVisiblePosts.length === 0) {
        document.getElementById('load-more-container').style.display = 'none';
      } else {
        document.getElementById('load-more-container').style.display = 'block';
      }
    }

    if (loadMoreBtn && hiddenPosts.length > 0) {
      loadMoreBtn.addEventListener('click', () => {
        hiddenPosts.forEach(post => {
          post.classList.remove('hidden');
        });
        document.getElementById('load-more-container').style.display = 'none';
      });
    } else if (hiddenPosts.length === 0) {
      document.getElementById('load-more-container').style.display = 'none';
    }
  </script>
